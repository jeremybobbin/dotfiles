#!/bin/sh
# TODO: divide into lambda support repo
main() {
	case $1 in 
		dialpad) $BROWSER https://dialpad.com/app 2>&1 > /dev/null & disown;;
		labs|love) lolless "$HOME/.local/share/art/ascii/lambda/$1";;
		orders) "$BROWSER" https://lambdalabs.com/tools/orders 2>&1 > /dev/null &;;
		ticket) $BROWSER "https://lambdalabs.zendesk.com/agent/tickets/$1" 2>&1 > /dev/null & disown;;
		zendesk) $BROWSER https://lambdalabs.zendesk.com/agent/dashboard 2>&1 > /dev/null  & disown;;
	esac
}
