defaults
auth           on
tls            on
tls_starttls   on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

account        jer.cx
host           jer.cx
port           587
from           jer@jer.cx
user           jer
passwordeval   "gpg --for-your-eyes-only --no-tty -qd $HOME/.passwords/password.gpg"

account        lambdal.com
host           smtp.gmail.com
port           587
from           jeremy@lambdal.com
user           jeremy@lambdal.com
passwordeval   "gpg --for-your-eyes-only --no-tty -qd $HOME/.passwords/jeremy@lambdal.com.gpg"

account default: lambdal.com
